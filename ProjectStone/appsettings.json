{
  "ConnectionStrings": {
    "DefaultConnection": "Server=[YourServerName];Database=[YourDatabaseName];Trusted_Connection=true;MultipleActiveResultSets=True;"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  // Store 3rd party API keys here. Use /Utility to access them. ("MailJetSettings")
  "MailJet": {
    "ApiKey": "[YourApiKey]",
    "ApiSecret": "[YourApiSecret]"
  },
  // 3rd Party API for handling payments
  "BrainTree": {
    "Environment": "sandbox",
    "MerchantId": "[YourMerchantId]",
    "PublicKey": "[YourPublicKey]",
    "PrivateKey": "[YourPrivateKey]"
  },
  // SSO Authorization
  "FaceBook": {
    "AppId": "[YourAppId]",
    "AppSecret": "[YourAppSecret]"
  },
  // SyncFusion License
  "SyncFusion": {
    "LicenseKey": "[YourLicenseKey]"
  } 
}
