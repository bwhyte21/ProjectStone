{
    "ConnectionStrings": {
        // Local DB.
        "DefaultConnection": "Server=ServerName;Database=database;Trusted_Connection=true;MultipleActiveResultSets=True;"
    },
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft": "Warning",
            "Microsoft.Hosting.Lifetime": "Information"
        }
    },
    "AllowedHosts": "*",
    // Store 3rd party API keys here. Use /Utility to access them. ("MailJetSettings")
    "MailJet": {
        "ApiKey": "[ApiKey]",
        "ApiSecret": "[ApiSecret]"
    },
    // 3rd Party API for handling payments
    "BrainTree": {
        "Environment": "[EnvironmentId]",
        "MerchantId": "[MerchantId]",
        "PublicKey": "[PublicKey]",
        "PrivateKey": "[PrivateKey]"
    },
    // SSO Authorization
    "FaceBook": {
        "AppId": "[AppId]",
        "AppSecret": "[AppSecret]"
    },
    // SyncFusion License
    "SyncFusion": {
        "LicenseKey": "[LicenseKey]"
    }
}